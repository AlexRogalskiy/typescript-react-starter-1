{
  "name": "@jmondi/webpack-starter",
  "description": "Webpack Starter Pack",
  "keywords": [
    "babel",
    "sass",
    "scss",
    "tailwindcss",
    "typescript",
    "webpack"
  ],
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:dev": "npm run clean && webpack --config build/webpack.conf.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.conf.js",
    "pretty": "prettier --write src/**/*.ts src/**/*.scss"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/helper-plugin-utils": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.2",
    "copy-webpack-plugin": "~4.5.1",
    "cross-env": "~5.1.4",
    "css-loader": "~0.28.11",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.4.0",
    "node-sass": "~4.8.3",
    "optimize-css-assets-webpack-plugin": "~4.0.0",
    "postcss-loader": "~2.1.4",
    "purgecss-webpack-plugin": "^1.0.2",
    "sass-loader": "~7.0.1",
    "style-loader": "~0.21.0",
    "tailwindcss": "~0.5.2",
    "ts-loader": "~4.2.0",
    "typescript": "~2.8.3",
    "webpack": "~4.6.0",
    "webpack-cli": "~2.0.15"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}
